{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/seo-helper": "^1.3",
        "barryvdh/laravel-translation-manager": "^0.5.0",
        "fideloper/proxy": "^4.0",
        "google/recaptcha": "^1.1",
        "intervention/image": "^2.3",
        "kalnoy/nestedset": "^4.2",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4",
        "maddhatter/laravel-fullcalendar": "^1.3",
        "mcamara/laravel-localization": "^1.2",
        "nwidart/laravel-modules": "^3.3",
        "rmccue/requests": "^1.7",
        "spatie/laravel-permission": "^2.9",
        "symfony/css-selector": "^3.2",
        "symfony/dom-crawler": "^3.2",
        "unisharp/laravel-filemanager": "^1.9",
        "yajra/laravel-datatables-oracle": "8.*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.3",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "jeroen-g/laravel-packager": "^1.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "orangehill/iseed": "^2.6",
        "phpunit/phpunit": "^7.0",
        "xethron/migrations-generator": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Aut\\": "Aut/"
        },
        "files": [
            "app/Library/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aut\\FileManager\\": "packages/Aut/FileManager/src",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
